{
  "name": "le-vinyle-backend",
  "version": "1.0.0",
  "description": "Backend for Le Vinyle - Musical session management application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "clean-db": "psql $DATABASE_URL -f db/init.sql",
    "setup-db": "npm run clean-db",
    "setup-playlists": "node scripts/setup-playlists.js",
    "migrate-multi-users": "node scripts/migrate-multi-users.js",
    "session-cleanup": "node scripts/session-maintenance.js",
    "session-cleanup-dry": "node scripts/session-maintenance.js --dry-run --verbose",
    "session-cleanup-quick": "node scripts/session-maintenance.js --inactive-hours 12 --delete-days 7",
    "session-stats": "node scripts/session-maintenance.js --dry-run --verbose",
    "session-cron": "node scripts/session-maintenance.js --cron",
    "apply-session-cleanup": "node scripts/apply-session-cleanup.js",
    "fix-permissions": "node scripts/fix-permissions.js",
    "diagnose": "node scripts/diagnose.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["music", "twitch", "spotify", "streaming", "session"],
  "author": "Le Vinyle Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-twitch-new": "^0.0.3",
    "pg": "^8.11.3",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
